<?xml version="1.0" encoding="UTF-8"?>
<component type="desktop">
    <id>com.github.lainsce.timetable</id>
    <metadata_license>CC0</metadata_license>
    <project_license>GPL-3.0+</project_license>
    <name>Timetable</name>
    <summary>Plot out your own timetable for the week and organize it</summary>
    <description>
        <p>Start organizing each day with the columns and export your timetable away in an easy way</p>
        <ul>
            <li>Intelligently start building your timetable with the day columns</li>
            <li>Quit anytime with the shortcut Ctrl + Q</li>
        </ul>
    </description>
    <provides>
        <binary>com.github.lainsce.timetable</binary>
    </provides>
    <developer_name>Lains</developer_name>
    <url type="homepage">https://github.com/lainsce/timetable/</url>
    <url type="bugtracker">https://github.com/lainsce/timetable/issues</url>
    <screenshots>
        <screenshot type="default">
            <image>https://raw.githubusercontent.com/lainsce/timetable/master/data/shot.png</image>
        </screenshot>
    </screenshots>
